<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="sanitySuit">
    <listeners>
        <listener class-name="UITestFramework.retryLogic.RetryListener"/>
        <listener class-name="Utils.Listeners.TestListener"/>
    </listeners>
    <parameter name="build" value="prod"/>
    <parameter name="level" value="payments"/>
    <parameter name="saucelabs" value="false"></parameter>
    <parameter name="methodName" value="sanity"></parameter>

    <test name="addMoneySanity">
        <groups>
            <run>
                <include name="setUp"/>
                <include name="instantiateHelpers"/>
                <include name="addMoneySanity"/>
                <include name="tearDown"/>
            </run>
        </groups>
        <classes>
            <class name="UITestFramework.CreateSession"/>
            <class name="UITestFramework.ExtentReport.Reporter"/>
            <class name="test.java.AddMoney.Test_AddMoney"/>
        </classes>
    </test>
</suite>