<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Sanity Suite A">

    <listeners>
        <listener class-name="Utils.Listeners.AnnotationTransformer"/>
    </listeners>

    <test preserve-order="true" name="Sanity Suite A">
        <parameter name="build" value="prod"/>
        <parameter name="level" value="payments"/>
        <parameter name="saucelabs" value="false"></parameter>
        <parameter name="codeCoverage" value="false"></parameter>
        <parameter name="androidOSVersion" value="12.0"></parameter>
        <parameter name="deviceName" value="deviceName"></parameter>
        <parameter name="udId" value="17228df2"></parameter>
        <parameter name="portNo" value="4723"></parameter>

        <groups>
            <run>
                <include name="setUp"/>
                <include name="sanity"/>
                <include name="tearDown"/>
            </run>
        </groups>

        <classes>

            <class name="Recharge.RechargeTest">
                <methods>
                    <exclude name="prepaid_Recharge"/>
                </methods>
            </class>
            <class name="ViewElectricityBill.ViewElectricityBillTest"/>
            <class name="ViewPipedGasBill.ViewPipedGasBillTest"/>
            <class name="HelpAndSupport.Test_HelpAndSupport"/>
            <class name="P2P.P2PTest"/>
            <class name="P2M.P2MTest"/>
            <class name="Checkout.CheckoutTest">
            <methods>
                <exclude name="Test04_zipAutoPayCheckoutElectricityBillPayment"/>
            </methods>
            </class>
            <class name="IMPSNew.IMPSTest"/>
            <class name="Gold.GoldTest"/>
            <class name="SecurityPin.SecurityPinTest"/>
            <class name="Loan.LoanTest"/>
            <class name="MoneyPlus.MoneyPlusTest"/>
            <class name="Rewards.RewardsTest"/>
            <class name="Offers.OfferTest"/>
            <class name="Profile.ProfileTest"/>
            <class name="MutualFunds.MutualFundsTest"/>
            <class name="PayRent.PayRentTest"/>

        </classes>
    </test>

</suite>
