<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Login Flow">

    <parameter name="build" value="prod"/>
    <parameter name="level" value="payments"/>
    <parameter name="saucelabs" value="false"></parameter>
    <parameter name="androidOSVersion" value="7.0"></parameter>

    <test verbose="2" preserve-order="true" name="Login flow Test">
        <groups>
            <run>
                <include name="setUp"/>
                <include name="instantiateHelpers"/>
                <include name="loginFlow"/>
                <include name="tearDown"/>
            </run>
        </groups>


        <classes>
            <class name="Utils.TestBase"/>
            <class name="Login.LoginTest"/>
            <class name="Utils.ExtentReport"></class>
        </classes>
    </test>

</suite>