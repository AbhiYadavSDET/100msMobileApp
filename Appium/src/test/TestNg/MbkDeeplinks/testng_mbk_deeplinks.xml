<suite name="DeepLink Suite">
    <listeners>
        <listener class-name="UITestFramework.retryLogic.RetryListener"/>
        <listener class-name="Utils.Listeners.TestListener"/>
        <listener class-name="Utils.ExtentReportListener"/>
    </listeners>
    <parameter name="build" value="prod"/>
    <parameter name="level" value="payments"/>
    <parameter name="saucelabs" value="false"></parameter>
    <parameter name="androidOSVersion" value="7.0"></parameter>

    <test name="Mobikwik Deeplinks Tests">
        <groups>
            <run>
                <include name="setUp"/>
                <include name="instantiateHelpers"/>
                <include name="Giftcard"/>
                <include name="tearDown"/>
            </run>
        </groups>

        <classes>
            <class name="UITestFramework.CreateSession"/>
            <class name="Utils.ExtentReport"/>
            <class name="MbkDeeplinks.Test_Mbk_Deep_Link"/>
        </classes>

    </test>
</suite>