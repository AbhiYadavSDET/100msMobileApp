<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression TRX Cases">

    <parameter name="build" value="prod"/>
    <parameter name="level" value="payments"/>
    <parameter name="saucelabs" value="false"></parameter>
    <parameter name="androidOSVersion" value="12.0"></parameter>


    <test verbose="2" preserve-order="true" name="Regression Test">
        <groups>
            <run>
                <include name="setUp"/>
                <include name="instantiateHelpers"/>
                <include name="regression"/>
                <include name="tearDown"/>
            </run>
        </groups>


        <classes>
            <class name="Utils.TestBase"/>
            <class name="AddMoney.Test_AddMoney"/>
            <class name="P2M.P2MTest"/>
            <class name="P2P.P2PTest"/>
            <class name="Gold.GoldTest"/>
            <class name="Recharge.RechargeTest">
                <methods>
                    <exclude name="prepaid_Recharge"/>
                </methods>
            </class>
            <class name="SecurityPin.SecurityPinTest"/>
        </classes>
    </test>

</suite>