<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Test Suite">

    <listeners>
        <listener class-name="Utils.Listeners.AnnotationTransformer"/>
    </listeners>

    <test preserve-order="true" name="Regression TRX Part A">
        <parameter name="build" value="prod"/>
        <parameter name="level" value="payments"/>
        <parameter name="saucelabs" value="false"></parameter>
        <parameter name="codeCoverage" value="false"></parameter>
        <parameter name="androidOSVersion" value="12.0"></parameter>
        <parameter name="deviceName" value="deviceName"></parameter>
        <parameter name="udId" value="17228df2"></parameter>
        <parameter name="portNo" value="4723"></parameter>

        <groups>
            <run>
                <include name="setUp"/>
                <include name="regression"/>
                <include name="tearDown"/>
            </run>
        </groups>

        <classes>
<!--            <class name="P2M.P2MTest"/>-->
            <class name="P2P.P2PTest"/>
<!--            <class name="IMPSNew.IMPSTest"/>-->
        </classes>
    </test>

