<suite name="Appium-Framework">
    <listeners>
        <listener class-name="UITestFramework.retryLogic.RetryListener"/>
        <listener class-name="UITestFramework.retryLogic.TestListener"/>
    </listeners>
    <parameter name="build" value="prod"/>
    <parameter name="level" value="payments"/>
    <parameter name="saucelabs" value="false"></parameter>
    <parameter name="androidOSVersion" value="7.0"></parameter>
    <parameter name="methodName" value="addmoneySanity"></parameter>


    <test name="Addmoney Sanity Suit">
        <groups>
            <run>
                <include name="setUp"/>
                <include name="instantiateHelpers"/>
                <include name="addmoneySanity"/>
                <include name="tearDown"/>

            </run>
        </groups>

        <classes>
            <class name="UITestFramework.CreateSession"/>
            <class name="IntegrationTests.AddMoney.AddmoneyTest"/>
            <class name="UITestFramework.ExtentReport.Reporter"/>
        </classes>
    </test>
</suite>