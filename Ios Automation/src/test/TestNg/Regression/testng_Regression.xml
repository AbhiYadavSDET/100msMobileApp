<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Regression Cases">

    <parameter name="build" value="prod"/>
    <parameter name="level" value="payments"/>
    <parameter name="saucelabs" value="false"></parameter>
    <parameter name="androidOSVersion" value="12.0"></parameter>

    <test verbose="2" preserve-order="true" name="Regression Test">
        <groups>
            <run>
                <include name="setUp"/>
                <include name="regression"/>
                <include name="tearDown"/>

            </run>
        </groups>


        <classes>
            <class name="Utils.TestBase"/>
            <class name="P2PExtra.P2PExtraTest"/>
            <class name="PipedGas.PipedGasTest"/>
            <class name="Recharge.RechargeTest"/>
            <class name="CCBP.CCBPTest"/>
            <class name="Offers.OffersTest"/>
            <class name="Electricity.ElectricityTest"/>
            <class name="AA.Test_AccountAggregator"/>
            <class name="PayRent.PayRentTest"/>
            <class name="CentralMapper.CentralMapperTest"/>
            <class name="IMPS.IMPSTest"/>
            <class name="P2P.P2PTest"/>
            <class name="P2M.P2MTest"/>

        </classes>
    </test>

</suite>