<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Login Flow">

<!--Listeners-->
<listeners>
    <listener class-name="Utils.ExtentReportListener"/>
</listeners>

<test verbose="2" preserve-order="true" name="Add-Money Test">
    <groups>
        <run>
            <include name="loginFlow"/>
        </run>
    </groups>


    <classes>
        <class name="LoginFlow.LoginTest"/>
        <class name="Utils.ExtentReport"/>
    </classes>
</test>

</suite>